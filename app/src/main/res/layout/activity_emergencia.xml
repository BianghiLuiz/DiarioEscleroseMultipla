        app:layout_constraintTop_toBottomOf="@+id/cheEmail" />

    <CheckBox
        android:id="@+id/cheFone3"
        android:layout_width="406dp"
        android:layout_height="30dp"
        android:layout_marginTop="45dp"
        android:text="Desequilíbrio"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.044"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/cheEmail" />

    <CheckBox
        android:id="@+id/cheFone2"
        android:layout_width="406dp"
        android:layout_height="30dp"
        android:layout_marginTop="8dp"
        android:text="Falta de coordenação dos movimentos"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.043"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/cheEmail" />

    <CheckBox
        android:id="@+id/cheFone11"
        android:layout_width="406dp"
        android:layout_height="30dp"
        android:layout_marginTop="332dp"
        android:text="Dificuldade para segurar objetos"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/cheEmail" />

    <CheckBox
        android:id="@+id/cheFone12"
        android:layout_width="406dp"
        android:layout_height="30dp"
        android:layout_marginTop="368dp"
        android:text="Dificuldade para escrever "
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/cheEmail" />

    <CheckBox
        android:id="@+id/cheFone13"
        android:layout_width="406dp"
        android:layout_height="30dp"
        android:layout_marginTop="404dp"
        android:text="Dificuldade para se concentrar"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/cheEmail" />

    <Button
        android:id="@+id/btnOk2"
        android:layout_width="182dp"
        android:layout_height="47dp"
        android:layout_marginBottom="68dp"
        android:background="@drawable/botao_vermelho"
        android:onClick="onClickRetornar"
        android:text="Voltar"
        android:textColor="@color/colorApp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.976"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/btnOk"
        android:layout_width="182dp"
        android:layout_height="47dp"
        android:layout_marginBottom="68dp"
        android:background="@drawable/botao_verde"
        android:text="Confirmar"
        android:textColor="@color/colorApp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.033"
        app:layout_constraintStart_toStartOf="parent" />


</androidx.constraintlayout.widget.ConstraintLayout>

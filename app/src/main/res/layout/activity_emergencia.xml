<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="br.com.visual.EscleroseMultipla.Activitys.EmergenciaActivity">

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="302dp"
        android:layout_height="83dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.041"
        app:srcCompat="@drawable/cm" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="CheckList"
        android:textColor="@color/colorTexto"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView2" />

    <CheckBox
        android:id="@+id/cheSMS"
        android:layout_width="406dp"
        android:layout_height="30dp"
        android:layout_marginTop="12dp"
        android:text="Dormência"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView2" />

    <CheckBox
        android:id="@+id/cheEmail"
        android:layout_width="406dp"
        android:layout_height="30dp"
        android:layout_marginTop="8dp"
        android:text="Tontura"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/cheSMS" />

    <CheckBox
        android:id="@+id/cheFone"
        android:layout_width="406dp"
        android:layout_height="30dp"
        android:layout_marginTop="116dp"
        android:text="Visão dupla"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/cheEmail" />

    <CheckBox
        android:id="@+id/cheFone5"
        android:layout_width="406dp"
        android:layout_height="30dp"
        android:layout_marginTop="152dp"
        android:text="Visão turva"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/cheEmail" />

    <CheckBox
        android:id="@+id/cheFone6"
        android:layout_width="406dp"
        android:layout_height="30dp"
        android:layout_marginTop="296dp"
        android:text="Nistagmo"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/cheEmail" />

    <CheckBox
        android:id="@+id/cheFone10"
        android:layout_width="406dp"
        android:layout_height="30dp"
        android:layout_marginTop="260dp"
        android:text="Fraqueza nos braços"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/cheEmail" />

    <CheckBox
        android:id="@+id/cheFone9"
        android:layout_width="406dp"
        android:layout_height="30dp"
        android:layout_marginTop="188dp"
        android:text="Fraqueza nas pernas"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/cheEmail" />

    <CheckBox
        android:id="@+id/cheFone7"
        android:layout_width="406dp"
        android:layout_height="30dp"
        android:layout_marginTop="224dp"
        android:text="Fraqueza em uma metade do corpo"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/cheEmail" />

    <CheckBox
        android:id="@+id/cheFone4"
        android:layout_width="406dp"
        android:layout_height="30dp"
        android:layout_marginTop="80dp"
        android:text="Fraqueza dos músculos da face"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.034"

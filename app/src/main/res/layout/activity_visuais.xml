        android:layout_width="135dp"
        android:layout_height="31dp"
        android:layout_marginStart="312dp"
        android:layout_marginLeft="312dp"
        android:layout_marginTop="244dp"
        android:max="5"
        android:rating="3.5"
        android:scaleX=".5"

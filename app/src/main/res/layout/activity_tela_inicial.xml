<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/fundo"

    tools:context=".Activitys.DataActivity">

    <TextView
        android:id="@+id/date"
        android:layout_width="406dp"
        android:layout_height="33dp"
        android:text="DIÁRIO DA"
        android:textAlignment="center"
        android:textColor="@color/colorTexto"
        android:textSize="25dp"
        android:visibility="visible"
        app:layout_constraintBottom_toTopOf="@+id/diario"
        app:layout_constraintHorizontal_bias="0.692"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.987" />

    <TextView
        android:id="@+id/diario"
        android:layout_width="406dp"
        android:layout_height="34dp"
        android:text="ESCLEROSE MÚLTIPLA"
        android:textAlignment="center"
        android:textColor="@color/colorTexto"
        android:textSize="25dp"
        android:visibility="visible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.571"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.281" />

    <Button
        android:id="@+id/btngoanotacoes"
        android:layout_width="283dp"
        android:layout_height="57dp"
        android:layout_marginTop="228dp"
        android:background="@drawable/botao_verde"
        android:text="Checklist"
        android:onClick="onClickCheckList"
        android:textColor="@color/colorApp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.34" />

    <Button
        android:id="@+id/btncalendario"
        android:layout_width="283dp"
        android:layout_height="57dp"
        android:layout_marginTop="228dp"
        android:background="@drawable/botao_verde"
        android:onClick="onClickCalendario"
        android:text="Calendário"
        android:textColor="@color/colorApp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btncalendario11"
        android:layout_width="283dp"
        android:layout_height="57dp"
        android:layout_marginTop="228dp"
        android:background="@drawable/botao_verde"
        android:onClick="onClickSintomas"
        android:text="Sintomas"
        android:textColor="@color/colorApp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.661" />

    <Button
        android:id="@+id/btnmenu"
        android:layout_width="283dp"
        android:layout_height="57dp"
        android:layout_marginTop="228dp"
        android:background="@drawable/botao_verde"
        android:onClick="onClickMenu"
        android:text="Menu"
        android:textColor="@color/colorApp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.183" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="302dp"
        android:layout_height="83dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.046"
        app:srcCompat="@drawable/cm" />

</androidx.constraintlayout.widget.ConstraintLayout>